@article{ ISI:000245815300048,
Author = {Kim, Myungbum and Khang, Nguyen and Woo, Insang and Choo, Hosung and
   Park, Ikmo},
Title = {{Small broadband disk-loaded monopole antenna with a vertical ground
   plane}},
Journal = {{MICROWAVE AND OPTICAL TECHNOLOGY LETTERS}},
Year = {{2007}},
Volume = {{49}},
Number = {{6}},
Pages = {{1401-1405}},
Month = {{JUN}},
Abstract = {{A small broadband monopole antenna with a vertical ground plane and an
   electromagnetically coupled feed is presented in this paper. The antenna
   was formed by positioning a probe with a folded stripline under a
   shorted square disk. A resonance of the shorted square disk was coupled
   to a resonance of the probe with a,folded stripline, so that the antenna
   had a wide bandwidth of about 37\% at the center frequency of 2.313 GHz
   based on a VSWR <= 2. The antenna had dimensions of 11 min x 17 mm x 11
   mm and rectangular slits were inserted into the vertical ground plane to
   improve distortion of the radiation pattern. (c) 2007 Wiley Periodicals,
   Inc.}},
