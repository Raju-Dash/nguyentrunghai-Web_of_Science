@article{ ISI:000395699400020,
Author = {Truong Khang Nguyen and Phuc Toan Dang and Park, Ikmo and Khai Quang Le},
Title = {{Broadband THz radiation through tapered semiconductor gratings on
   high-index substrate}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA B-OPTICAL PHYSICS}},
Year = {{2017}},
Volume = {{34}},
Number = {{3}},
Pages = {{583-589}},
Month = {{MAR 1}},
Abstract = {{We numerically demonstrate an extraordinary optical transmission (EOT)
   through semiconductor gratings with plasmonic properties on a high-index
   substrate over a broad terahertz (THz) bandwidth at the plasmonic
   Brewster channel. The THz EOT is due to impedance matching, an
   inherently non-resonant mechanism, at the grating entrance and exit
   faces, which are periodically carved with tapered slits. The optimal
   grating geometry provides a transmission of over 85\% over a broad
   0.2-1.0 THz bandwidth at a Brewster angle of incidence of 75 degrees. In
   addition, we introduce a perfect THz absorber with low-loss and
   metal-free properties over a broad operating bandwidth based on the
   plasmonic Brewster transmission concept. (C) 2017 Optical Society of
   America}},
