@article{ ISI:000453045600024,
Author = {Huy Hung Tran and Tuan Tu Le and Truong Khang Nguyen},
Title = {{Dual-band dual-sense circularly polarized antenna for S- and C-band
   applications}},
Journal = {{MICROWAVE AND OPTICAL TECHNOLOGY LETTERS}},
Year = {{2019}},
Volume = {{61}},
Number = {{1}},
Pages = {{141-146}},
Month = {{JAN}},
Abstract = {{This paper presents a dual-band dual-sense circularly polarized (CP)
   antenna for S- and C-band applications. A semicircular patch and a
   circular slot are first designed to achieve wide operation bandwidth in
   the C-band with right-hand CP (RHCP), and then, a curved slot is
   properly inserted around the circular slot for S-band realization with
   left-hand CP (LHCP). Final antenna prototype has an overall size of 0.32
   lambda(L) x 0.32 lambda(L) x 0.01 lambda(L), where lambda(L) is
   free-space wavelength at the lowest CP frequency. Experimental results
   show that good CP characteristics for both RHCP and LHCP can be
   achieved. The measured -10 dB impedance bandwidth is from 2.2 to 6.5
   GHz, equivalent to about 98.8\%. Additionally, the measured 3 dB
