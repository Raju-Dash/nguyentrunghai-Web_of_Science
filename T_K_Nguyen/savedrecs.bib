@article{ ISI:000494485000047,
Author = {Phuc Toan Dang and Le, Khai Q. and Lee, Ji-Hoon and Truong Khang Nguyen},
Title = {{A Designed Broadband Absorber Based on ENZ Mode Incorporating Plasmonic
   Metasurfaces}},
Journal = {{MICROMACHINES}},
Year = {{2019}},
Volume = {{10}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{In this paper, we present a numerical study of a metamaterial absorber
   that provides polarization-insensitive absorption over a broad bandwidth
   of operation over the mid-infrared region. The absorber consists of a
   periodically patterned metal-dielectric-metal structure integrated with
   an epsilon-near-zero (ENZ) nanolayer into the insulating dielectric gap
   region. Such an anomalous broadband absorber is achieved thanks to a
   couple of resonant modes including plasmon and ENZ modes that are
   excited under mid-IR light illumination. By adding a 0.06-mu m-thick ENZ
   layer between the patterned gold rectangular grating and the SiO2
   dielectric layer, the absorber captures >95\% light over a 1.5 mu m
   bandwidth centered at a near-8-mu m wavelength over a wide range of
   oblique incidence under transverse-magnetic and -electric polarizations.
   The designed ENZ-based wideband absorber has potential for many
   practical applications, including sensing, imaging and solar energy
   harvesting over a wide frequency regime.}},
