@article{ ISI:000437194900002,
Author = {Son Xuat Ta and Minh Duc Nguyen and Thi Hai-Yen Nguyen and Truong Khang
   Nguyen and Khac Kiem Nguyen and Chien Dao-Ngoc},
Title = {{A low-profile +/- 45o dual-polarized antenna based on metasurface and
   its arrays for base station applications}},
Journal = {{JOURNAL OF ELECTROMAGNETIC WAVES AND APPLICATIONS}},
Year = {{2018}},
Volume = {{32}},
Number = {{14}},
Pages = {{1748-1767}},
Abstract = {{This paper presents a low-cost, low-profile,+/- 45o dual-polarized
   antenna based on metasurface for base station applications. The antenna
   consists of two +/- 45o printed dipoles, which are fed by novel printed
   baluns for allowing broad operational bandwidth and a high port-to-port
   isolation. The antenna is backed by a metallic cavity reflector to
   obtain the broadside radiation. The metasurface, which is a periodic
   lattice of square patches printed on a thin low-cost substrate, is
   inserted between the radiator and the reflector to achieve the
   low-profile. The single element with an overall size of 160mm x 160mm x
   16mm (0.96(o) x 0.96(o) x 0.096(o) at 1.8GHz) yields a voltage standing
   wave ratio (VSWR) < 2 bandwidth of>29\% for both ports, an isolation
   of>34dB, a gain of>8.4, and a radiation efficiency of>80\%. Also, a
   four-element array based on the proposed antenna is fed by non-uniform
   feeding networks to achieve a low sidelobe level (SLL). The array
   yielded an isolation of>20dB, a gain of>11.7 dBi, and an SLL of<-20dB
   for both +/- 45o polarizations.}},
